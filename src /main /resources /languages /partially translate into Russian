      - '&bTip: use &e/itemstorage delete <id>&b, to delete an item'
    params: '<id>'
  show:
    description:
      - '&b&lOpen a gui with your saved items'
      - ''
      - '&e[page] &bpage of the gui, by default 1'
    params: '[page]'
